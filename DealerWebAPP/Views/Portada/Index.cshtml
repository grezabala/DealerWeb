@model DealerWebApp.DealertWeb.Models.MPortadas

<!--Service-->
<section class="page-section" id="services">
    <div class="container">
        <div class="text-center">
            <h4 class="section-heading text-uppercase">NUESTROS SERVICIOS</h4>
            <h4 class="section-subheading text-muted"></h4>
        </div>
        <div class="row text-center">
            <div class="col-md-4">
                @*  fas fa-circle fa-stack-2x*@
                <span class="fa-stack fa-4x">
                    <i class="fa fa-circle fa-stack-2x text-primary"></i>
                    <i class="fa fa-thermometer-empty fa-stack-1x fa-inverse" aria-hidden="true"></i>
                    @*<i class="fas fa-shopping-cart fa-stack-1x fa-inverse"></i>*@
                </span>
                <h5 class="my-3">CAMBIO DE ACEITE & FILTRO</h5>
                <p class="text-muted"></p>
            </div>
            <div class="col-md-4">
                <span class="fa-stack fa-4x">
                    <i class="fas fa-circle fa-stack-2x text-primary"></i>
                    <i class="fas fa-laptop fa-stack-1x fa-inverse"></i>
                </span>
                <h5 class="my-3">REVISION COMPUTARIZADA</h5>
                <p class="text-muted"></p>
            </div>
            <div class="col-md-4">
                <span class="fa-stack fa-4x">
                    <i class="fas fa-circle fa-stack-2x text-primary"></i>
                    @*   <i class="fas fa-lock fa-stack-1x fa-inverse"></i>*@
                    <i class="fa fa-car fa-stack-1x fa-inverse" aria-hidden="true"></i>
                </span>
                <h5 class="my-3">INSTALACION DE LUZ LED & SONIDO</h5>
                <p class="text-muted"></p>
            </div>
            <div class="col-md-4">
                <span class="fa-stack fa-4x">
                    <i class="fas fa-circle fa-stack-2x text-primary"></i>
                    <i class="fa fa-car fa-stack-1x fa-inverse" aria-hidden="true"></i>
                </span>
                <h5 class="my-3">BALANCEO</h5>
                <p class="text-muted"></p>
            </div>
            <div class="col-md-4">
                <span class="fa-stack fa-4x">
                    <i class="fas fa-circle fa-stack-2x text-primary"></i>
                    <i class="fa fa-life-ring fa-stack-1x fa-inverse" aria-hidden="true"></i>
                </span>
                <h5 class="my-3">SISTEMA DE FRENO</h5>
                <p class="text-muted"></p>
            </div>
            <div class="col-md-4">
                <span class="fa-stack fa-4x">
                    <i class="fas fa-circle fa-stack-2x text-primary"></i>
                    <i class="fa-thin fa-screwdriver-wrench fa-stack-2x fa-inverse" aria-hidden="true"></i>
                    <i class="fa fa-wrench fa-stack-1x fa-inverse" aria-hidden="true"></i>
                </span>
                <h5 class="my-3">DESABOLLADURA & PINTURA</h5>
                <p class="text-muted"></p>
            </div>
            <!--INFORME DE GARANTIA-->
            <div class="col-md-12">
                <p class="text-muted">
                    Todos nuestro servicios son realizado por un personal altamente capacitado, con más de cinco
                    años de experiencia.
                </p>
            </div>
        </div>
    </div>
</section>
