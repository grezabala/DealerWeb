                            </div>
                            <img class="img-fluid" src="@mBrands.UrlImg" alt="" />
                        </a>
                        <div class="portfolio-caption" style="border-radius:10px !important;">
                            <div class="portfolio-caption-heading" @*style="font-size: 20px;"*@>@mBrands.NameBrand</div>
                            <div class="portfolio-caption-subheading text-muted">Catalogo de la Marca.</div>
                        </div>
                    </div>
                </div>
            }
        </div>

